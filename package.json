{
    "name": "rekrutteringsbistand-kandidatsok",
    "version": "0.1.0",
    "scripts": {
        "start": "cross-env vite",
        "start:export": "cross-env VITE_EXPORT=true vite",
        "start:mock": "cross-env VITE_MOCK=true vite",
        "start:mock:export": "cross-env VITE_MOCK=true VITE_EXPORT=true vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prepare": "husky install"
    },
    "dependencies": {
        "@navikt/ds-css": "^1.0.0-rc.2",
        "@navikt/ds-icons": "^1.0.0-rc.2",
        "@navikt/ds-react": "^1.0.0-rc.2",
        "@navikt/navspa": "^6.0.1",
        "history": "^5.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^5.3.3"
    },
    "devDependencies": {
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^1.3.2",
        "cross-env": "^7.0.3",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.7.4",
        "vite": "^2.9.13"
    },
    "browserslist": {
        "production": [
            "last 3 chrome version",
            "last 3 firefox version"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always",
        "printWidth": 100
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier"
        ]
    }
}
